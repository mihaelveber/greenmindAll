# Production Environment Variables
DEBUG=False
DJANGO_SETTINGS_MODULE=config.settings
ALLOWED_HOSTS=api.greenmindai.net,app.greenmindai.net
CORS_ALLOWED_ORIGINS=https://app.greenmindai.net,https://admin.greenmindai.net

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Celery
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Database - will be overridden by secrets
DATABASE_NAME=greenmindai_prod
DATABASE_USER=postgres
DATABASE_HOST=postgres
DATABASE_PORT=5432

# Frontend API URL
VITE_API_URL=https://api.greenmindai.net/api
